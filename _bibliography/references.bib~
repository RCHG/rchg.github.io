@ARTICLE{Oshchepkov2013,
  author = {Sergey Oshchepkov and Andrey Bril and Tatsuya Yokota and Yukio Yoshida
	and Thomas Blumenstock and Nicholas M Deutscher and Susanne Dohe
	and Ronald Macatangay and Isamu Morino and Justus Notholt and Markus
