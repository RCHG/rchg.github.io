@ARTICLE{Suto2013,
  author = {Hiroshi Suto and Jun Yoshida and Raphael Desbiens and Takahiro Kawashima
	and Akihiko Kuze},
  title = {Characterization and correction of spectral distortions induced by
	microvibrations onboard the GOSAT Fourier transform spectrometer.},
  journal = {Appl Opt},
  year = {2013},
  volume = {52},
  pages = {4969--4980},
  number = {20},
  month = {Jul},
  abstract = {Microvibrations onboard greenhouse gases observing satellite (GOSAT)
	cause scan speed variations in the TANSO Fourier transform spectrometer.
	The associated periodic sampling errors generate ghost features in
	O2 A-band spectra, where surface pressure and aerosol properties
	are retrieved to determine the optical path through the atmosphere.
	A correction algorithm has been developed to re-compute the interferograms
	at equally spaced sampling intervals. The key is to determine iteratively
	the amplitude and phase of sinusoidal perturbations with predetermined
	frequencies to minimize the magnitude of the out-of-band ghosts artifacts
	after correction of the sampling grid. This correction algorithm
	drastically reduces errors in retrieved surface pressure and improves
	agreement with ground-based observations.},
  language = {eng},
  medline-pst = {ppublish},
  owner = {ramiro},
  pii = {259050},
  pmid = {23852213},
  timestamp = {2013.08.09}
}

